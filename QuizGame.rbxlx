function ConnectionTable(scripts, ...)
    task.spawn(function(...)
        preload(scripts, ...)
        task.wait()
        Start(scripts, ...)
    end, ...)

    if scripts.closing and RunService:IsServer() then
        game:BindToClose(function()
            task.spawn(scripts.closing, scripts)
        end)
    end

end

