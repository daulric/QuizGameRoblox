end

function Start(scripts, ...)
    xpcall(task.spawn, err, scripts.start, scripts, ... )
end

function preload(scripts, ...)
    if scripts.preload then
        xpcall(task. spawn, err, scripts.preload, scripts, ...)
    end
end

